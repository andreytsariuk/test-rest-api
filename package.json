{
  "name": "test-rest-api",
  "version": "1.0.0",
  "description": "api ",
  "private": true,
  "scripts": {
    "postinstall": ". ./scripts/post_install.sh",
    "build": "./node_modules/eslint/bin/eslint.js .",
    "preversion": "echo test",
    "version": "git add .",
    "postversion": "git push --no-verify && git push --tags --no-verify && echo \"\n======Version Pushed Successfully=====\n\" ",
    "start": "node ./bin/www",
    "echo-version": "echo $npm_package_version",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "npm run build && npm run unit-tests && npm run integration-tests",
    "integration-tests": "nyc --reporter=lcov --reporter=text-summary mocha --timeout 100000 --bail --exit --full-trace --async-only ./test/integration-tests/",
    "unit-tests": "nyc mocha --bail --exit --full-trace  ./test/unit-tests/"
  },
  "dependencies": {
    "aws-sdk": "^2.469.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "bookshelf": "^0.12.0",
    "bookshelf-soft-delete": "^4.0.0",
    "config": "^1.30.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "handlebars": "^4.0.11",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.2.0",
    "knex": "^0.13.0",
    "lodash": "^4.17.13",
    "moment": "^2.20.1",
    "nodemailer": "^4.6.4",
    "nodemailer-ses-transport": "^1.5.1",
    "pg": "^7.4.0",
    "rand-token": "^0.4.0",
    "shortid": "^2.2.8",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0",
    "winston-elasticsearch": "^0.7.12",
    "zipcode-to-timezone": "0.0.9"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cli-color": "^1.4.0",
    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "mocha": "^5.1.1",
    "nyc": "^14.1.1",
    "winston-console-formatter": "^1.0.0-beta.3"
  }
}